PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX sh: <http://www.w3.org/ns/shacl#>
PREFIX nen2660: <https://w3id.org/nen2660/def#>

# Retrieve Fysieke plaat/location URIs and their corresponding names

SELECT DISTINCT ?uri ?naamNL ?naamEN
WHERE {
  ?uri a owl:Class .
  ?uri rdfs:subClassOf ?type .
  FILTER(?type IN (nen2660:RealObject, nen2660:SpatialRegion)) .
  ?uri skos:prefLabel ?naamNL .
  FILTER (LANG(?naamNL) = "nl") . 
  FILTER(?naamNL != "actor"@nl) .
  ?uri skos:prefLabel ?naamEN .
  FILTER (LANG(?naamEN) = "en") . 
}
